<mujoco model="vbot">
  <compiler angle="radian" meshdir="assets/" />
  <option cone="elliptic" impratio="100" />

  <custom>
    <numeric data="30" name="max_contact_points"/>
    <numeric data="12" name="max_geom_pairs"/>
  </custom>

  <default>
    <material specular="0" shininess="0.25"/>
    <default class="vita">
      <geom friction="0.4" margin="0.001" condim="3" />
      <joint axis="0 1 0" damping="0.1" armature="0.0043" frictionloss="0.2" />
      <motor forcerange="-17 17" />
      <default class="R_hip_joint">
        <joint axis="1 0 0" range="-0.73304 0.73304" limited="true" />
      </default>
      <default class="L_hip_joint">
        <joint axis="1 0 0" range="-0.73304 0.73304" limited="true" />
      </default>
      <default class="F_thigh_joint">
        <joint range="-1.559 3.1298" limited="true" />
      </default>
      <default class="R_thigh_joint">
        <joint range="-0.51181 4.177" limited="true" />
      </default>
      <default class="calf_joint">
        <joint range="-2.6387 -0.7854" limited="true" damping="0.1" armature="0.04" frictionloss="0.2" />
        <motor forcerange="-34 34" />
      </default>
      <default class="visual">
        <geom type="mesh" contype="0" conaffinity="0" group="2" density="0" />
      </default>
      <default class="collision">
        <geom group="3" rgba="0.752941 0.752941 0.752941 1" />
        <default class="foot">
          <geom size="0.021" quat="0.707107 -0.707107 0 0" pos="0 0 0"
                solimp="0.9 .99 0.001" priority="10" condim="3" />
        </default>
      </default>
    </default>
  </default>

  <asset>
    <mesh name="base_link" file="base_link.STL" />
    <mesh name="FR_hip" file="FR_hip.STL" />
    <mesh name="FR_thigh" file="FR_thigh.STL" />
    <mesh name="FR_calf" file="FR_calf.STL" />
    <mesh name="FR_foot" file="FR_foot.STL" />
    <mesh name="FL_hip" file="FL_hip.STL" />
    <mesh name="FL_thigh" file="FL_thigh.STL" />
    <mesh name="FL_calf" file="FL_calf.STL" />
    <mesh name="FL_foot" file="FL_foot.STL" />
    <mesh name="RR_hip" file="RR_hip.STL" />
    <mesh name="RR_thigh" file="RR_thigh.STL" />
    <mesh name="RR_calf" file="RR_calf.STL" />
    <mesh name="RR_foot" file="RR_foot.STL" />
    <mesh name="RL_hip" file="RL_hip.STL" />
    <mesh name="RL_thigh" file="RL_thigh.STL" />
    <mesh name="RL_calf" file="RL_calf.STL" />
    <mesh name="RL_foot" file="RL_foot.STL" />
    <mesh name="head_yaw" file="head_yaw.STL" />
    <mesh name="head_pitch" file="head_pitch.STL" />
  </asset>

  <worldbody>
    <body name="trunk" pos="0 0 0.462" childclass="vita">
      <camera name="track" pos="0.846 -1.3 0.316" xyaxes="0.866 0.500 0.000 -0.171 0.296 0.940" mode="trackcom"/>
      <camera name="top" pos="-1 0 1" xyaxes="0 -1 0 0.7 0 0.7" mode="trackcom"/>
      <camera name="side" pos="0 -2 1" xyaxes="1 0 0 0 1 2" mode="trackcom"/>
      <inertial
          pos="0.026103 0.000278 0.024686"
          mass="9.016326"
          fullinertia="0.060632 0.144345 0.125819 -0.000200 -0.033265 -0.000044"
      />
      <freejoint />
      <site name="imu" pos="0 0 0" />
      <site name="head" pos="0.3 0 0" rgba="1 0 0 1" size="0.02" group="5"/>

      <!-- Head visual -->
      <geom type="mesh" class="visual" pos="0.17289 0 0.12752"
            rgba="0.498039 0.498039 0.498039 1" mesh="head_yaw" />
      <geom type="mesh" class="visual" pos="0.17289 0 0.15552"
            rgba="0.75294 0.75294 0.75294 1" mesh="head_pitch" />
      <geom name="collision_head_box" class="collision"
            size="0.084602 0.078997 0.041873" pos="0.18389 0 0.16552" type="box"
            rgba="0.498039 0.498039 0.498039 1" />

      <!-- Trunk -->
      <geom type="mesh" rgba="0.752941 0.752941 0.752941 1" mesh="base_link" class="visual" />
      <geom name="trunk" size="0.133329 0.101205 0.068913" pos="0 0 0.004583"
            type="box" class="collision" />

      <!-- FR leg -->
      <body name="FR_hip" pos="0.18453 -0.051 0">
        <inertial pos="-0.027882 0.015355 0" quat="-0.137609 0.678417 0.233155 0.682973"
                  mass="0.079643" diaginertia="8.72171e-05 7.7874e-05 2.29089e-05" />
        <joint name="FR_hip_joint" class="R_hip_joint" />
        <geom type="mesh" rgba="0.898039 0.917647 0.929412 1" mesh="FR_hip" class="visual" />
        <body name="FR_thigh" pos="0 -0.0975 0">
          <inertial pos="-0.000498 0.038065 -0.024467" quat="0.975282 -0.219111 0.024734 0.0142991"
                    mass="1.55038" diaginertia="0.00679074 0.00586921 0.00204705" />
          <joint name="FR_thigh_joint" class="F_thigh_joint" />
          <geom type="mesh" rgba="0.498039 0.498039 0.498039 1" mesh="FR_thigh" class="visual" />
          <geom size="0.0129095 0.013 0.06282" pos="-0.029488 -0.000724 -0.10287"
                quat="0.999071 0 -0.0430946 0" type="box" class="collision" />
          <geom size="0.044 0.038455" pos="0 0.02351 -0.000177"
                quat="-0.707107 0.707107 0 0" type="cylinder" class="collision" />
          <body name="FR_calf" pos="0 0 -0.1985">
            <inertial pos="0.00261631 0 -0.125302" quat="0.706944 -0.0151852 -0.0151852 0.706944"
                      mass="0.183986" diaginertia="0.00125035 0.0012368 3.55485e-05" />
            <joint name="FR_calf_joint" class="calf_joint" />
            <geom type="mesh" rgba="0.498039 0.498039 0.498039 1" mesh="FR_calf" class="visual" />
            <geom size="0.01 0.009 0.06" pos="0.00376 0 -0.099894"
                  quat="0.994686 0 -0.102956 0" type="box" class="collision" />
            <geom pos="0 0 -0.214" quat="1 0 0 0" type="mesh"
                  rgba="0.498039 0.498039 0.498039 1" mesh="FR_foot" class="visual" />
            <geom name="FR_foot" class="foot" pos="0 0 -0.214" />
            <site name="FR" pos="0 0 -0.214" type="sphere" size="0.021" group="5"/>
          </body>
        </body>
      </body>

      <!-- FL leg -->
      <body name="FL_hip" pos="0.18453 0.051 0">
        <inertial pos="-0.027882 -0.015355 0" quat="0.137609 0.678417 -0.233155 0.682973"
                  mass="0.079643" diaginertia="8.72171e-05 7.7874e-05 2.29089e-05" />
        <joint name="FL_hip_joint" class="L_hip_joint" />
        <geom type="mesh" rgba="1 1 1 1" mesh="FL_hip" class="visual" />
        <body name="FL_thigh" pos="0 0.0975 0">
          <inertial pos="-0.000498 -0.038065 -0.024467" quat="0.975282 0.219111 0.024734 -0.0142991"
                    mass="1.55038" diaginertia="0.00679074 0.00586921 0.00204705" />
          <joint name="FL_thigh_joint" class="F_thigh_joint" />
          <geom type="mesh" rgba="0.498039 0.498039 0.498039 1" mesh="FL_thigh" class="visual" />
          <geom size="0.0129095 0.013 0.06282" pos="-0.029488 0.000724 -0.10287"
                quat="0.999071 0 -0.0430946 0" type="box" class="collision" />
          <geom size="0.044 0.038455" pos="0 -0.02351 -0.000177"
                quat="-0.707107 0.707107 0 0" type="cylinder" class="collision" />
          <body name="FL_calf" pos="0 0 -0.1985">
            <inertial pos="0.00261631 0 -0.125302" quat="0.706944 -0.0151852 -0.0151852 0.706944"
                      mass="0.183986" diaginertia="0.00125035 0.0012368 3.55485e-05" />
            <joint name="FL_calf_joint" class="calf_joint" />
            <geom type="mesh" rgba="0.498039 0.498039 0.498039 1" mesh="FL_calf" class="visual" />
            <geom size="0.01 0.009 0.06" pos="0.00376 0 -0.099894"
                  quat="0.994686 0 -0.102956 0" type="box" class="collision" />
            <geom pos="0 0 -0.214" quat="1 0 0 0" type="mesh"
                  rgba="0.498039 0.498039 0.498039 1" mesh="FL_foot" class="visual" />
            <geom name="FL_foot" class="foot" pos="0 0 -0.214" />
            <site name="FL" pos="0 0 -0.214" type="sphere" size="0.021" group="5"/>
          </body>
        </body>
      </body>

      <!-- RR leg -->
      <body name="RR_hip" pos="-0.18453 -0.051 0">
        <inertial pos="0.027882 0.015355 0" quat="0.233155 0.682973 -0.137609 0.678417"
                  mass="0.079643" diaginertia="8.72171e-05 7.7874e-05 2.29089e-05" />
        <joint name="RR_hip_joint" class="R_hip_joint" />
        <geom type="mesh" rgba="0.898039 0.917647 0.929412 1" mesh="RR_hip" class="visual" />
        <body name="RR_thigh" pos="0 -0.0975 0">
          <inertial pos="-0.000498 0.038065 -0.024467" quat="0.975282 -0.219111 0.024734 0.0142991"
                    mass="1.55038" diaginertia="0.00679074 0.00586921 0.00204705" />
          <joint name="RR_thigh_joint" class="R_thigh_joint" />
          <geom type="mesh" rgba="0.498039 0.498039 0.498039 1" mesh="RR_thigh" class="visual" />
          <geom size="0.0129095 0.013 0.06282" pos="-0.029488 -0.000724 -0.10287"
                quat="0.999071 0 -0.0430946 0" type="box" class="collision" />
          <geom size="0.044 0.038455" pos="0 0.02351 -0.000177"
                quat="-0.707107 0.707107 0 0" type="cylinder" class="collision" />
          <body name="RR_calf" pos="0 0 -0.1985">
            <inertial pos="0.00261631 0 -0.125302" quat="0.706944 -0.0151852 -0.0151852 0.706944"
                      mass="0.183986" diaginertia="0.00125035 0.0012368 3.55485e-05" />
            <joint name="RR_calf_joint" class="calf_joint" />
            <geom type="mesh" rgba="0.498039 0.498039 0.498039 1" mesh="RR_calf" class="visual" />
            <geom size="0.01 0.009 0.06" pos="0.00376 0 -0.099894"
                  quat="0.994686 0 -0.102956 0" type="box" class="collision" />
            <geom pos="0 0 -0.214" quat="1 0 0 0" type="mesh"
                  rgba="0.498039 0.498039 0.498039 1" mesh="RR_foot" class="visual" />
            <geom name="RR_foot" class="foot" pos="0 0 -0.214" />
            <site name="RR" pos="0 0 -0.214" type="sphere" size="0.021" group="5"/>
          </body>
        </body>
      </body>

      <!-- RL leg -->
      <body name="RL_hip" pos="-0.18453 0.051 0">
        <inertial pos="0.027882 -0.015355 0" quat="-0.233155 0.682973 0.137609 0.678417"
                  mass="0.079643" diaginertia="8.72171e-05 7.7874e-05 2.29089e-05" />
        <joint name="RL_hip_joint" class="L_hip_joint" />
        <geom type="mesh" rgba="1 1 1 1" mesh="RL_hip" class="visual" />
        <body name="RL_thigh" pos="0 0.0975 0">
          <inertial pos="-0.000498 -0.038065 -0.024467" quat="0.975282 0.219111 0.024734 -0.0142991"
                    mass="1.55038" diaginertia="0.00679074 0.00586921 0.00204705" />
          <joint name="RL_thigh_joint" class="R_thigh_joint" />
          <geom type="mesh" rgba="0.498039 0.498039 0.498039 1" mesh="RL_thigh" class="visual" />
          <geom size="0.0129095 0.013 0.06282" pos="-0.029488 0.000724 -0.10287"
                quat="0.999071 0 -0.0430946 0" type="box" class="collision" />
          <geom size="0.044 0.038455" pos="0 -0.02351 -0.000177"
                quat="-0.707107 0.707107 0 0" type="cylinder" class="collision" />
          <body name="RL_calf" pos="0 0 -0.1985">
            <inertial pos="0.00261631 0 -0.125302" quat="0.706944 -0.0151852 -0.0151852 0.706944"
                      mass="0.183986" diaginertia="0.00125035 0.0012368 3.55485e-05" />
            <joint name="RL_calf_joint" class="calf_joint" />
            <geom type="mesh" rgba="0.498039 0.498039 0.498039 1" mesh="RL_calf" class="visual" />
            <geom size="0.01 0.009 0.06" pos="0.00376 0 -0.099894"
                  quat="0.994686 0 -0.102956 0" type="box" class="collision" />
            <geom pos="0 0 -0.214" quat="1 0 0 0" type="mesh"
                  rgba="0.498039 0.498039 0.498039 1" mesh="RL_foot" class="visual" />
            <geom name="RL_foot" class="foot" pos="0 0 -0.214" />
            <site name="RL" pos="0 0 -0.214" type="sphere" size="0.021" group="5"/>
          </body>
        </body>
      </body>
    </body>
  </worldbody>

  <actuator>
    <!-- FR -->
    <motor class="vita" name="FR_hip" joint="FR_hip_joint" ctrllimited="true" ctrlrange="-17 17" />
    <motor class="vita" name="FR_thigh" joint="FR_thigh_joint" ctrllimited="true" ctrlrange="-17 17" />
    <motor class="calf_joint" name="FR_calf" joint="FR_calf_joint" ctrllimited="true" ctrlrange="-34 34" />
    <!-- FL -->
    <motor class="vita" name="FL_hip" joint="FL_hip_joint" ctrllimited="true" ctrlrange="-17 17" />
    <motor class="vita" name="FL_thigh" joint="FL_thigh_joint" ctrllimited="true" ctrlrange="-17 17" />
    <motor class="calf_joint" name="FL_calf" joint="FL_calf_joint" ctrllimited="true" ctrlrange="-34 34" />
    <!-- RR -->
    <motor class="vita" name="RR_hip" joint="RR_hip_joint" ctrllimited="true" ctrlrange="-17 17" />
    <motor class="vita" name="RR_thigh" joint="RR_thigh_joint" ctrllimited="true" ctrlrange="-17 17" />
    <motor class="calf_joint" name="RR_calf" joint="RR_calf_joint" ctrllimited="true" ctrlrange="-34 34" />
    <!-- RL -->
    <motor class="vita" name="RL_hip" joint="RL_hip_joint" ctrllimited="true" ctrlrange="-17 17" />
    <motor class="vita" name="RL_thigh" joint="RL_thigh_joint" ctrllimited="true" ctrlrange="-17 17" />
    <motor class="calf_joint" name="RL_calf" joint="RL_calf_joint" ctrllimited="true" ctrlrange="-34 34" />
  </actuator>

  <sensor>
    <gyro site="imu" name="gyro"/>
    <velocimeter site="imu" name="local_linvel"/>
    <framepos objtype="site" objname="imu" name="position"/>
    <framelinvel objtype="site" objname="imu" name="global_linvel"/>
    <frameangvel objtype="site" objname="imu" name="global_angvel"/>
    <framequat objtype="site" objname="imu" name="orientation"/>
    <framelinvel objtype="site" objname="FR" name="FR_global_linvel"/>
    <framelinvel objtype="site" objname="FL" name="FL_global_linvel"/>
    <framelinvel objtype="site" objname="RR" name="RR_global_linvel"/>
    <framelinvel objtype="site" objname="RL" name="RL_global_linvel"/>
    <framepos objtype="site" objname="FR" name="FR_pos" reftype="site" refname="imu"/>
    <framepos objtype="site" objname="FL" name="FL_pos" reftype="site" refname="imu"/>
    <framepos objtype="site" objname="RR" name="RR_pos" reftype="site" refname="imu"/>
    <framepos objtype="site" objname="RL" name="RL_pos" reftype="site" refname="imu"/>
    <framepos objtype="site" objname="head" name="head_pos"/>
  </sensor>
</mujoco>
